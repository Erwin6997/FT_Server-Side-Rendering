<div class="container">
    {{#if this.message}}
    <div class="error">
    <h1>{{this.message}}</h1>
            <p>
        Suggestions:
        </p>
        <ul>
            <li>Make sure that all words are spelled correctly.</li>
            <li>Try different keywords.</li>
            <li>Try more general keywords.</li>
            <li>Try fewer keywords.</li>
        </ul>
    </div>
    {{/if}}
    {{#each finalResults}}
    <div class="containerNews">
        <div class="containerTitle"><a href="{{this.location.uri}}" class="title">{{this.title.title}}</a></div>
        <div class="containerSummery">
            <p class="summery">{{this.summary.excerpt}}</p>
        </div>
        <div class="containerEditorial"><a href="{{this.location.uri}}"
                class="subheading">{{this.editorial.subheading}}</a></div>
        <div class="containerByline">
            {{#if this.editorial.byline}}
            <p class="byline">{{this.editorial.byline}}</p>
            {{else}}
            <p class="byline"></p>
            {{/if}}
            <div class="containerDate">
                <p class="date">{{timeDate this.lifecycle.initialPublishDateTime}}</p>
            </div>
        </div>
    </div>
    {{/each}}
</div>